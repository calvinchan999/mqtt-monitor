{
  "name": "mqtt-monitor",
  "version": "1.0.0",
  "description": "MQTT monitoring tool with Web UI and real-time message monitoring",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "proxy": "node proxy-server.js",
    "start-all": "concurrently \"npm start\" \"npm run proxy\"",
    "dev-all": "concurrently \"npm run dev\" \"npm run proxy\""
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "mqtt": "^5.3.4",
    "node-fetch": "^2.6.7",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "mqtt",
    "monitor",
    "websocket",
    "real-time"
  ],
  "author": "",
  "license": "MIT"
}
